[
  {
    "id": "water_lines_cliff_line_triangles",
    "type": "line",
    "source": "osm",
    "source-layer": "water_lines",
    "minzoom": 15,
    "maxzoom": 24,
    "filter": [
      "all",
      [
        "in",
        [
          "get",
          "type"
        ],
        [
          "literal",
          [
            "cliff"
          ]
        ]
      ],
      [
        "!",
        [
          "in",
          [
            "get",
            "surface"
          ],
          [
            "literal",
            [
              "water"
            ]
          ]
        ]
      ]
    ],
    "layout": {
      "line-cap": "butt",
      "line-join": "miter",
      "visibility": "visible"
    },
    "paint": {
      "line-color": "rgba(153, 153, 153, 1)",
      "line-translate-anchor": "viewport",
      "line-width": 3,
      "line-pattern": "cliff-8",
      "line-offset": 2
    }
  }
]