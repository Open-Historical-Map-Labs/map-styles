[
  {
    "id": "roads_proposed",
    "type": "line",
    "source": "osm",
    "source-layer": "transport_lines",
    "minzoom": 12,
    "maxzoom": 24,
    "filter": [
      "all",
      [
        "in",
        [
          "get",
          "type"
        ],
        [
          "literal",
          [
            "proposed"
          ]
        ]
      ],
      [
        "!",
        [
          "in",
          [
            "get",
            "class"
          ],
          [
            "literal",
            [
              "railway"
            ]
          ]
        ]
      ]
    ],
    "layout": {
      "visibility": "visible",
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#ffffff",
      "line-width": [
        "interpolate",
        [
          "exponential",
          1.5
        ],
        [
          "zoom"
        ],
        12,
        1.5,
        18,
        12
      ],
      "line-dasharray": [
        1,
        2
      ]
    }
  }
]